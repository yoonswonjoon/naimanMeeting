<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--외부-->
    <item android:state_checked="false">
        <shape>
            <size
                android:width="25dp"
                android:height="25dp"/>
            <corners
                android:radius="4dp"
                />
            <solid
                android:color="#00004D40"/>
            <stroke
                android:width="1dp"
                android:color="#323232"/>
        </shape>
    </item>

    <item android:state_checked="true">
        <layer-list>
            <item >
                <shape>
                    <size
                        android:width="25dp"
                        android:height="25dp"/>
                    <corners
                        android:radius="4dp"
                        />
                    <solid
                        android:color="#00004D40"/>
                    <stroke
                        android:width="1dp"
                        android:color="#323232"/>
                </shape>
            </item>

            <item android:drawable="@drawable/checkbox_checkicon"
                >


            </item>

        </layer-list>
    </item>


    <!--내부-->

</selector>